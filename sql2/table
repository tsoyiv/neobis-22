CREATE TABLE cars_shop (
	car_id INT NOT NULL AUTO_INCREMENT,
	car_name VARCHAR(50) NOT NULL,
	date_of_publicaion DATE NOT NULL,
	max_speed INT NOT NULL,
	type_of_car VARCHAR(30) NOT NULL,
	PRIMARY KEY(car_id)
	);
	
CREATE TABLE bmw (
	id INT,
     	year INT NOT NULL,
     	name VARCHAR(30),
    	FOREIGN KEY(id) REFERENCES cars_shop(car_id)
     	);
